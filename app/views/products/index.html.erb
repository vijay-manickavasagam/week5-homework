<div class="page-header">
  <h1>Product Catalog</h1>
</div>

<% @product_list.each do |product| %>
  <div class="row">
    <div class="col-md-4">
      <a href="/products/show"><img src=<%= product.image %>></a>
    </div>
    <div class="col-md-8">
      <h3><a href="/products/show"><%= product.name %></a></h3>
      <!-- <p class="lead"></p> -->
      <p><%= product.description %></p>
    </div>
  </div>
<% end %>
